pipeline {
    agent any
    stages {
        stage("init") {
            steps {
                script {
                   echo "Initialization...";
                }
            }
        }
        stage("build") {
            steps {
                script {
                    echo "Building the App...";
                }
            }
        }
        stage("deploy") {
            steps {
                script {
                    echo "App deployed successfully...";
                }
            }
        }
    }   
}
